<div>
  <h4>Items</h4>
   <h5>
    <a href='/items/new'>Create New Item</a>
  </h5>
</div>
<div>
  <% @items.each do |item| %>
    <h4>
      <a href="/items/<%= item.id %>"><%= item.title %></a>
    </h4>
    <img src="/widget_1.jpg">
    <h5>Price: <%= number_to_currency(item.unit_price) %> </h5>
  <% end %>
</div>
